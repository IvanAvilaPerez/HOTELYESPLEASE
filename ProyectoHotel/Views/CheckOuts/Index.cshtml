@model IEnumerable<ProyectoHotel.Models.CheckOut>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaCheckOut)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdReservaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCheckOut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdReservaNavigation.IdReserva)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCheckOut">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCheckOut">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCheckOut">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
